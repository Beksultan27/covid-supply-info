[Unit]
Description=Keystone backend
After=network.target

[Service]
Environment=NODE_PORT=3000
Environment=NODE_ENV=production
Type=simple
User=nodejs
ExecStart=/usr/bin/node /opt/covid-supply/node_modules/.bin/keystone start
WorkingDirectory=/opt/covid-supply
Restart=on-failure

[Install]
WantedBy=multi-user.target

